#include<algorithm>
#include<iostream>
#include<vector>
using namespace std;


int fact(int n)
{
	if(n==0)
	return 1;
	else
	{
//	int k=n*fact(n-1);
//	cout<<k<<" ";
	return n*fact(n-1);
	}
}

int main()
{
	int n;
	cout<<"enter n"<<endl;
	cin>>n;
	int s=fact(n);
	cout<<"factorial is : "<<s<<endl;
	return 0;
}
