#include <iostream>
using namespace std;

void search(int c[],int n,int m)
{   int flag=0;
    for(int i=0;i<n;i++)
    {
        if(c[i]==m)
        {   
            cout<<i<<"\n";
            return;
        }
    }
cout<<"-1\n";
    
}



int main() {
    
    int t;
    cin>>t;
    while(t--)
    {
        
        int n,c[100],m;
        cin>>n;
        for(int i=0;i<n;i++)
        {
            cin>>c[i];   
        }
        cin>>m;
    
       
        search(c,n,m);
        
    }
	
	return 0;
}
